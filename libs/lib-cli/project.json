{
  "name": "lib-cli",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "libs/lib-cli/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/lib-cli",
        "main": "libs/lib-cli/src/index.ts",
        "tsConfig": "libs/lib-cli/tsconfig.lib.json",
        "assets": ["libs/lib-cli/*.md"]
      }
    },
    "demo-build": {
      "executor": "@nrwl/esbuild:esbuild",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/libs/lib-cli-demo",
        "format": ["cjs"],
        "main": "libs/lib-cli/src/demo.setup.ts",
        "tsConfig": "libs/lib-cli/tsconfig.lib.json"
      }
    },
    "demo": {
      "executor": "@nrwl/js:node",
      "options": {
        "buildTarget": "lib-cli:demo-build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["libs/lib-cli/**/*.ts"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "libs/lib-cli/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    }
  },
  "tags": []
}
